{
  "name": "mimmi20/json-normalizer",
  "type": "library",
  "description": "An additional normalizer for ergebnis/json-normalizer",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/json-normalizer",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/json-normalizer/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "ext-json": "*",
    "ergebnis/json-normalizer": "~0.11.0 || ~0.12.0 || ~0.13.0",
    "mimmi20/json-class": "~2.0.13"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "~2.8.0",
    "ergebnis/phpstan-rules": "~0.15.1",
    "infection/infection": "~0.13.6 || ~0.17.5",
    "mimmi20/coding-standard": "~1.0.3",
    "pepakriz/phpstan-exception-rules": "~0.11.1",
    "phpstan/extension-installer": "~1.0.5",
    "phpstan/phpstan": "~0.12.47",
    "phpstan/phpstan-deprecation-rules": "~0.12.5",
    "phpstan/phpstan-phpunit": "~0.12.16",
    "phpstan/phpstan-strict-rules": "~0.12.5",
    "phpunit/phpunit": "~7.5.20 || ~8.5.8 || ~9.3.8"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Json\\Normalizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JsonTest\\Normalizer\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/json-normalizer/issues",
    "source": "https://github.com/mimmi20/json-normalizer"
  }
}
