{
  "name": "mimmi20/json-normalizer",
  "type": "library",
  "description": "An additional normalizer for ergebnis/json-normalizer",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/json-normalizer",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/json-normalizer/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ergebnis/json-normalizer": "^1.0.3",
    "mimmi20/json-class": "^4.0.0"
  },
  "require-dev": {
    "mimmi20/coding-standard": "^2.5.1",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^1.2.0",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-phpunit": "^1.0.0",
    "phpunit/phpunit": "^9.5.10",
    "rector/rector": "^0.12.5"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Json\\Normalizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JsonTest\\Normalizer\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/json-normalizer/issues",
    "source": "https://github.com/mimmi20/json-normalizer"
  }
}
