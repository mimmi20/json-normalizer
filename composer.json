{
  "name": "mimmi20/json-normalizer",
  "type": "library",
  "description": "An additional normalizer for ergebnis/json-normalizer",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/json-normalizer",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/json-normalizer/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "ergebnis/json-normalizer": "^0.11.0 || ^0.12.0",
    "mimmi20/json-class": "~2.0.9"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.3.2 || ^2.4.0",
    "ergebnis/phpstan-rules": "^0.14.4",
    "friendsofphp/php-cs-fixer": "^2.16.3",
    "infection/infection": "^0.13.6 || ^0.16.3",
    "pepakriz/phpstan-exception-rules": "^0.10.1",
    "phpstan/phpstan": "^0.12.19",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-phpunit": "^0.12.8",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "phpunit/phpunit": "^7.5.20 || ^8.5.4 || ^9.1.3",
    "squizlabs/php_codesniffer": "^3.5.5"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Json\\Normalizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JsonTest\\Normalizer\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/json-normalizer/issues",
    "source": "https://github.com/mimmi20/json-normalizer"
  }
}
